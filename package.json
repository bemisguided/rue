{
  "name": "rue",
  "version": "0.8.1",
  "description": "nodejs dependency injection container",
  "main": "dist/rue.js",
  "engines": {
    "node": ">=6.11.1"
  },
  "keywords": [
    "rue",
    "rue framework",
    "ioc",
    "inversion of control",
    "di",
    "dependency injection"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "flow": "flow",
    "prepare": "npm run build",
    "build": "npm run build-babel && npm run build-flow",
    "build-babel": "npm run clean && babel ./lib --out-dir dist/ --copy-files",
    "build-flow": "flow-copy-source ./lib ./dist",
    "test": "npm run lint && npm run flow && jest --coverage",
    "test-watch": "jest --watch",
    "lint": "eslint .",
    "lint-fix": "eslint --fix ."
  },
  "repository": {
    "url": "http://github.com/bemisguided/rue.git",
    "type": "git"
  },
  "author": "@bemisguided",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bemisguided/rue/issues"
  },
  "homepage": "https://github.com/bemisguided/rue#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.48.0",
    "flow-copy-source": "^1.2.0",
    "jest": "^20.0.4",
    "q": "^1.5.0",
    "rimraf": "^2.6.1"
  },
  "files": [
    "index.js",
    "dist"
  ],
  "false": {},
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "lib/**.{js}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html"
    ]
  }
}
