{
  "name": "rue",
  "version": "0.0.7",
  "description": "nodejs dependency injection container",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "prepublish": "npm run build",
    "build": "npm run clean && babel ./lib --out-dir dist/ --copy-files",
    "test": "flow && mocha --compilers js:babel-core/register test/**/**.spec.js",
    "flow": "flow",
    "jscs": "jscs lib/**/*.js test/**/*.js"
  },
  "repository": {
    "url": "http://github.com/bemisguided/rue.git",
    "type": "git"
  },
  "author": "@bemisguided",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bemisguided/rue/issues"
  },
  "homepage": "https://github.com/bemisguided/rue#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.0.2",
    "flow-bin": "^0.48.0",
    "jscs": "^3.0.7",
    "mocha": "^2.3.4",
    "rimraf": "^2.6.1"
  },
  "entries": {
    "eventemitter2": "^0.4.14",
    "q": "^2.0.3 "
  },
  "files": [
    "index.js",
    "lib"
  ]
}
