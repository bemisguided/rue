{
  "name": "rue",
  "version": "0.0.7",
  "description": "nodejs dependency injection container",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "clean": "rimraf dist/ && rm -fr coverage",
    "flow": "flow",
    "prepare": "npm run build",
    "build": "npm run clean && babel ./lib --out-dir dist/ --copy-files",
    "test": "npm run flow && jest test --coverage",
    "test-watch": "npm run flow && jest test --watch",
    "jscs": "jscs lib/**/*.js test"
  },
  "repository": {
    "url": "http://github.com/bemisguided/rue.git",
    "type": "git"
  },
  "author": "@bemisguided",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bemisguided/rue/issues"
  },
  "homepage": "https://github.com/bemisguided/rue#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.48.0",
    "jest": "^20.0.4",
    "jscs": "^3.0.7",
    "rimraf": "^2.6.1"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "false": {}
}
